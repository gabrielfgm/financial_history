  author = {Pietryka, Matthew T. and DeBats, Donald A.},
  year = {2017},
  journal = {American Political Science Review},
  volume = {111},
  number = {2},
  pages = {360--378},
  issn = {15375943},
  doi = {10.1017/S000305541600071X},
  abstract = {\textbackslash textlessp\textbackslash textgreaterIndividual-level studies of electoral turnout and vote choice have focused largely on personal attributes as explanatory variables. We argue that scholars should also consider the social network in which individuals are embedded, which may influence voting through variation in individuals' social proximity to elites. Our analysis rests on newly discovered historical records revealing the individual votes of all electors in the 1859 statewide elections in Alexandria, Virginia and the 1874 municipal elections in Newport, Kentucky, paired with archival work identifying the social relations of the cities' populations. We also replicate our core findings using survey data from a modern municipal election. We show that individuals more socially proximate to elites turn out at a higher rate and individuals more socially proximate to a given political party's elites vote disproportionately for that party. These results suggest an overlooked social component of voting and provide a rare nineteenth-century test of modern voting theories.\textbackslash textless/p\textbackslash textgreater},
  isbn = {0003055416000}
}

