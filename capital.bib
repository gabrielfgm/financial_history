  keywords = {Commerce -- History.,International economic relations -- Religious aspects -- History.,Religion -- Economic aspects -- History.},
  file = {/Users/gabrielmesevage/Zotero/storage/YSRCC8UR/Trivellato et al. - 2014 - Religion and Trade Cross-Cultural Exchanges in Wo.pdf;/Users/gabrielmesevage/Zotero/storage/S3T62WRW/detail.html}
