@article{spanosFrequentistInterpretationProbability2013,
  title = {A Frequentist Interpretation of Probability for Model-Based Inductive Inference},
  author = {Spanos, Aris},
  year = {2013},
  month = jun,
  journal = {Synthese},
  volume = {190},
  number = {9},
  pages = {1555--1585},
  issn = {1573-0964},
  doi = {10.1007/s11229-011-9892-x},
  abstract = {The main objective of the paper is to propose a frequentist interpretation of probability in the context of model-based induction, anchored on the Strong Law of Large Numbers (SLLN) and justifiable on empirical grounds. It is argued that the prevailing views in philosophy of science concerning induction and the frequentist interpretation of probability are unduly influenced by enumerative induction, and the von Mises rendering, both of which are at odds with frequentist model-based induction that dominates current practice. The differences between the two perspectives are brought out with a view to defend the model-based frequentist interpretation of probability against certain well-known charges, including [i] the circularity of its definition, [ii] its inability to assign `single event' probabilities, and [iii] its reliance on `random samples'. It is argued that charges [i]\textendash [ii] stem from misidentifying the frequentist `long-run' with the von Mises collective. In contrast, the defining characteristic of the long-run metaphor associated with model-based induction is neither its temporal nor its physical dimension, but its repeatability (in principle); an attribute that renders it operational in practice. It is also argued that the notion of a statistical model can easily accommodate non-IID samples, rendering charge [iii] simply misinformed.}
}

